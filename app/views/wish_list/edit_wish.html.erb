<h1>編輯許願卡</h1>

<%= form_for @wish_list , url: update_wish_card_info_path(@wish_list) , method:"patch" do |f| %> 
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <%= f.submit %>

<% end %>